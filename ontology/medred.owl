# http://w3id.org/medred/medred#Choice

:Choice a owl:Class ;
	rdfs:comment "Each Choice corresponds to a possible answer to a given Question"@en ;
	rdfs:label "Choice for a Question"@en .
# 
# http://w3id.org/medred/medred#ChoiceList

:ChoiceList a owl:Class ;
	rdfs:subClassOf rdf:List , _:genid1 .

_:genid1 a owl:Restriction ;
	owl:onProperty rdf:first ;
	owl:allValuesFrom :Choice .

:ChoiceList rdfs:subClassOf _:genid2 .

_:genid2 a owl:Restriction ;
	owl:onProperty rdf:rest ;
	owl:allValuesFrom _:genid3 .

_:genid3 a owl:Class ;
	owl:unionOf _:genid7 .

_:genid7 a rdf:List ;
	rdf:first :ChoiceList ;
	rdf:rest _:genid4 .

_:genid4 a rdf:List ;
	rdf:first _:genid5 .

_:genid5 a owl:Class ;
	owl:oneOf _:genid6 .

_:genid6 a rdf:List ;
	rdf:first rdf:nil .

rdf:nil a owl:NamedIndividual .

_:genid6 rdf:rest rdf:nil .

_:genid4 rdf:rest rdf:nil .

:ChoiceList rdfs:comment "The ChoiceList corresponds to a list of Choice items for a given Question"@en ;
	rdfs:label "List of Choice elements"@en .
